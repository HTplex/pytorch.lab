   "source": [
    "x = Variable(torch.ones(5, 3), requires_grad=True)\n",
    "print(x)\n",
    "y = x + 2\n",
    "print(y)\n",
    "print(y.grad_fn)\n",
    "z = y*y+3\n",
    "output = z.mean()\n",
